
class Node{
    int data;
    Node next;
    
    Node(int a){
        data =a;
        next = null;
    }
    public void getData(){
        System.out.println(data);
    }
    public void getNext(){
        System.out.println(next);
    }
    
}
class Main {
    public static void main(String[] args) {
        Node a = new Node(70);
       a.getData();
       a.getNext();
       System.out.println(a);
    }
}
